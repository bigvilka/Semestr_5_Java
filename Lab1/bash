
javac -sourcepath src -d bin -classpath /home/vladis/IdeaProjects/Lab1/commons-codec-1.8.jar src/com/Matveev/hash/Main.java src/com/Matveev/hash/CountHash.java src/com/Matveev/hash/InteractiveMode.java src/com/Matveev/hash/SimpleMode.java


java -classpath commons-codec-1.8.jar:bin com.Matveev.hash.Main -i /home/vladis/IdeaProjects/Lab1/i.webp


jar cmf manifest.mf main.jar Main.class CountHash.class InteractiveMode.class SimpleMode.class

java -jar main.jar -md5 -f /home/vladis/IdeaProjects/Lab1/i.webp



